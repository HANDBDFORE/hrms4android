<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <!-- 选中时的图片背景 -->
    <item android:drawable="@drawable/button_press_corner" android:state_pressed="true"/>
    <!-- 获得焦点时的图片背景 -->
    <item android:drawable="@drawable/button_press_corner" android:state_focused="true"/>
    <!-- 默认背景 -->
    <item android:drawable="@drawable/button_corner"/>

</selector>
